    <!-- Engine Left exterior -->
    <crank>
      <name>engstart1</name>
      <path>Aircraft/Br-761/Sounds/starter.wav</path>
      <property>/engines/engine[0]/cranking</property>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>2.0</factor>
