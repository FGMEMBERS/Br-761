        <min>0.15</min>
        <max>2.0</max>
        <offset>-0.0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
      <position>
        <x> -2.627 </x>
        <y>  1.740 </y>
        <z>  0.021 </z>
      </position>
    </engine>
    
