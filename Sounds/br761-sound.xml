        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    
    <engine>
      <name>engine1_idle</name>
      <mode>looped</mode>
      <path>Aircraft/Br-761/Sounds/running-slow.wav</path>
      <condition>
        <and>
          <property>engines/engine[0]/running</property>
          <less-than>
            <property>engines/engine[0]/rpm</property>
            <value>1700</value>
          </less-than>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/mp-osi</property>
        <factor>0.009</factor>
        <min>0.15</min>
        <max>0.6</max>
        <offset>0.15</offset>
      </volume>
      <pitch>
        <property>engines/engine[0]/rpm</property>
        <factor>0.0004</factor>
        <min>0.1</min>
        <max>2.0</max>
        <offset>0.3</offset>
      </pitch>
      <position>
        <x> -3.10 </x>
        <y>  8.35 </y>
        <z> -0.30 </z>
      </position>
      <reference-dist>20.0</reference-dist>
      <max-dist>50.0</max-dist>
    </engine>
    
    <engine>
      <name>engineLeft</name>
      <mode>looped</mode>
      <path>Aircraft/Br-761/Sounds/running.wav</path>
      <condition>
        <and>
          <property>/engines/engine[0]/running</property>
          <not>
            <property>/sim/current-view/internal</property>
          </not>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/mp-osi</property>
        <factor>0.00375</factor>
        <min>0.5</min>
        <max>2.0</max>
      </volume>
